<?xml version="1.0" encoding="UTF-8"?>
DNI REGEX
<xs:element name="dni">
    <xs:simpleType>
        <xs:restriction base="xs:string">
            <xs:pattern
                value="\d{8}[a-zA-Z]" />
        </xs:restriction>
    </xs:simpleType>
</xs:element>
-----------------------------------------------------------------------
EMAIL REGEX
<xs:element name="email">
    <xs:simpleType>
        <xs:restriction base="xs:string">
            <xs:pattern
                value="[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,}" />
        </xs:restriction>
    </xs:simpleType>
</xs:element>
------------------------------------------------------------------------
PA Q EMPIECE MAYUS
<xs:element name="nombre">
    <xs:simpleType>
        <xs:restriction base="xs:string">
            <xs:pattern
                value="[A-ZÁÉÍÓÚÑ].*" />
        </xs:restriction>
    </xs:simpleType>
</xs:element>
